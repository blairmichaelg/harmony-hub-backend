# Environment
NODE_ENV=test
PORT=3001
LOG_LEVEL=debug

# Database
DATABASE_URL=postgresql://test_user:test_password@localhost:5432/harmony_hub_test_db

# Redis
REDIS_URL=redis://localhost:6379/1

# Authentication
JWT_SECRET=test_jwt_secret_key_for_testing_purposes_only

# AWS
AWS_ACCESS_KEY_ID=test_aws_access_key_id
AWS_SECRET_ACCESS_KEY=test_aws_secret_access_key
AWS_REGION=us-west-2

# AI Services
OPENAI_API_KEY=test_openai_api_key
GOOGLE_CLOUD_PROJECT_ID=test_google_cloud_project_id
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/test-google-credentials.json

# Audio Processing
FFMPEG_PATH=/usr/local/bin/ffmpeg

# Storage
S3_BUCKET_NAME=test-bucket-name

# Feature Flags
ENABLE_NEW_COLLABORATION_FEATURE=true

# Localization
DEFAULT_LANGUAGE=en

# Performance
ENABLE_CACHING=false

# Security
ENABLE_SSL=false
CORS_ALLOWED_ORIGINS=http://localhost:3001

# Logging
LOG_FILE_PATH=/tmp/harmony-hub-test.log

# Server
MAX_CONNECTIONS=50

# Cache
CACHE_TTL=60
